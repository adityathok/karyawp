{
    "version": 2,
    "settings": {
      "appearanceTools": true,
      "spacing": {
        "blockGap": true,
        "margin": true,
        "padding": true,
        "units": ["%","px","em","rem","vh","vw"]
      },
      "layout": {
        "contentSize": "800px",
        "wideSize": "1200px",
        "fullSize": "none"
      },
      "typography": {
        "customFontSize": true,
        "customLineHeight": true,
        "dropCap": true,
        "fontSizes": [
          {
            "size": "0.85rem",
            "name": "Small",
            "slug": "small"
          },
          {
            "size": "1rem",
            "name": "Medium",
            "slug": "medium"
          },
          {
            "size": "1.5rem",
            "name": "Large",
            "slug": "large"
          },
          {
            "size": "2rem",
            "name": "Extra Large",
            "slug": "x-large"
          }
        ]
      },
      "blocks": {
        "core/group": {
          "spacing": {
            "margin": true,
            "padding": true
          }
        },
        "core/site-title": {
          "spacing": {
            "margin": true,
            "padding": true
          }
        }
     },
     "styles": {
       "elements": {
         "link": {
             "typography": {
               "textDecoration": "none"
             }
         }
       }
     }
    }
  }